generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id              String         @id
  email           String         @unique
  user_meta_data  Json?          @default("{}")
  created_at      DateTime       @default(now())
  updated_at      DateTime       @updatedAt
  first_name      String?
  last_name       String?
}